{
  "name": "Wishlist",
  "version": "1.0",
  "permissions": ["storage", "activeTab"],
  "description": "Changes the wishlist",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": ["*://*.tommy.com/*"],
      "run_at": "document_idle",
      "js": ["modal.bundle.js"]
    }
    {
      "matches": ["*://*.tommy.com/wishlist*"],
      "run_at": "document_idle",
      "js": ["wishlist.bundle.js"]
    }
  ]
}

